{
  "manifest_version": 2,
  "name": "Welcome Kiosk | Tower Grove",
  "short_name": "Welcome-TG",
  "description": "Welcome Center app for The Journey Tower Grove",

  "version": "0.0.2.0",

  "icons": {
    "16": "assets/img/icon_16.png",
    "128": "assets/img/icon_128.png"
  },

  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "webview",
    "clipboardRead",
    "clipboardWrite",
    "power"
  ],
  "kiosk_enabled" : true
}
